# Sequoia General Trading Portfolio - Environment Variables

# App Configuration
REACT_APP_SITE_NAME=Sequoia General Trading
REACT_APP_SITE_URL=https://sequoiatrading.com
REACT_APP_SITE_DESCRIPTION=Your trusted partner in import and export services across Ghana and West Africa

# Company Information
REACT_APP_COMPANY_EMAIL=sequoia.trades@outlook.com
REACT_APP_COMPANY_PHONE=+233 55 388 2284
REACT_APP_COMPANY_ADDRESS=Ghana

# API Configuration (if you plan to add backend services)
REACT_APP_API_BASE_URL=https://api.sequoiatrading.com
REACT_APP_API_VERSION=v1

# Third-party Services (uncomment and configure as needed)
# REACT_APP_GOOGLE_ANALYTICS_ID=GA-XXXXXXXXX
# REACT_APP_GOOGLE_MAPS_API_KEY=your_google_maps_api_key
# REACT_APP_RECAPTCHA_SITE_KEY=your_recaptcha_site_key

# Email Service (for contact form)
# REACT_APP_EMAILJS_SERVICE_ID=your_emailjs_service_id
# REACT_APP_EMAILJS_TEMPLATE_ID=your_emailjs_template_id
# REACT_APP_EMAILJS_PUBLIC_KEY=your_emailjs_public_key

# Social Media Links
# REACT_APP_FACEBOOK_URL=https://facebook.com/sequoiatrading
# REACT_APP_TWITTER_URL=https://twitter.com/sequoiatrading
# REACT_APP_LINKEDIN_URL=https://linkedin.com/company/sequoia-general-trading
# REACT_APP_INSTAGRAM_URL=https://instagram.com/sequoiatrading

# Feature Flags
REACT_APP_ENABLE_ANIMATIONS=true
REACT_APP_ENABLE_DARK_MODE=false
REACT_APP_ENABLE_MULTILINGUAL=false

# Development
REACT_APP_DEBUG_MODE=false
GENERATE_SOURCEMAP=true

# Build Configuration
BUILD_PATH=build
PUBLIC_URL=/

# Performance
REACT_APP_LAZY_LOAD_IMAGES=true
REACT_APP_PRELOAD_CRITICAL_ASSETS=true